{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["next/core-web-vitals", "plugin:react/recommended", "standard", ""next/typescript""],
    "parserOptions": {
      "ecmaFeatures": {
        "tsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "eqeqeq": "off",
      "multiline-ternary": "off"
    }
  }